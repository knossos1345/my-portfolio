datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
  provider = "prisma-client-js"
}

model Tree {
  id    String @id @default(uuid()) @map("id") @db.Uuid
  water Int
  views Int
}